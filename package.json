{
  "name": "elo7-api",
  "version": "0.1.0",
  "description": "Alternative Elo7 API",
  "main": "dist/index.js",
  "scripts": {
    "watch": "nodemon src/app.js --exec babel-node",
    "build": "rm -rf dist/ && babel src/ --out-dir dist/",
    "start": "node dist/app.js",
    "test": "eslint src/ tests/ && nyc jest --verbose --coverage"
  },
  "jest": {
    "verbose": true,
    "testMatch": [
      "**/tests/**/*.spec.js"
    ],
    "collectCoverage": true
  },
  "author": "Ricardo Ledo de Tulio <ledo.tulio@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-preset-es2015": "^6.24.1",
    "coveralls": "^2.13.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.6.0",
    "jest": "^20.0.4",
    "nodemon": "^1.11.0",
    "nyc": "^10.3.2",
    "regenerator-runtime": "^0.10.5"
  },
  "dependencies": {
    "bluebird": "^3.5.0",
    "body-parser": "^1.17.2",
    "elo7": "^0.1.0",
    "express": "^4.15.3",
    "ramda": "^0.24.1"
  }
}
